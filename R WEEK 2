f<-function(q) (q^5)  #TASK 3 function of q
a<-c(-5,4,12,17,19)   # domain 
b<-f(a)               #range (result)
plot(a,b)             # plotting graph 

rm(list=ls())

x<-c(1,4,5,6)        #TASK 4 objectives of x 
y<-c(4,6,7,8)        # objectives of y 

data1<-data.frame(x,y)  #create data1 for x and y 
data1<-data.frame (gdp=x, inflation=y) # naming x and y 
plot(data1$gdp, data1$inflation) # plotting gdp and inflation

rm(list=ls())
