x<-c(1,4,5,6)  #TASK1 objectives of x 
y<-c(4,6,7,8)  # objectives of y 

data1<-data.frame(x,y) #creating data1 for x and y 
plot(data1$x,data1$y)  #plotting x and y 

rm(list=ls())              #removing objectives 
ls()

